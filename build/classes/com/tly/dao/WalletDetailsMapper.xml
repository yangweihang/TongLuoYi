<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- 钱包明细 -->
<mapper namespace="com.tly.dao.IWalletDetailsDao">
	<!-- 新增 -->
	<insert id="insert" parameterType="com.tly.model.WalletDetails">
		insert into walletdetails(wxid,summoney,money,txmoney,balance) values(#{wxid},#{summoney},#{money},#{txmoney},#{balance})
	</insert>
</mapper>